{
  "name": "swiper",
  "displayName": "swiper",
  "description": "Emacs style swiper fuzzy search buffer/window",
  "version": "2.1.1",
  "publisher": "wenhoujx",
  "author": {
    "name": "Wenshuai Hou",
    "email": "wenhoujx@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/wenhoujx/swiper",
  "repository": {
    "type": "git",
    "url": "https://github.com/wenhoujx/swiper"
  },
  "icon": "img/swiper-icon.jpeg",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:swiper.swiper",
    "onCommand:swiper.swiper.swiper-word-at-cursor"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "swiper.swiper",
        "title": "Swiper Search/Resume",
        "category": "Swiper"
      },
      {
        "command": "swiper.swiper-word-at-cursor",
        "title": "Swiper Search at Cursor",
        "category": "Swiper"
      }
    ],
    "configuration": {
      "title": "Swiper",
      "properties": {
        "swiper.searchFromCursor": {
          "type": "boolean",
          "default": false,
          "description": "Start searching from the cursor position instead of the beginning of the file"
        },
        "swiper.selectMatch": {
          "type": "boolean",
          "default": true,
          "description": "Select the picked match text by swiper"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.2.0"
  }
}
